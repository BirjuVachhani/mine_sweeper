{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter: Launch main.dart",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "Dev",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "env": {
        "Dev": "true",
        "FLAVOR" : "dev"
      }
    },
    {
      "name": "Prod App",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "env": {
        "PROD": "true",
        "FLAVOR" : "prod"
      }
    }
  ]
}
